{
    "name": "envant/eloquent-lockable",
    "description": "Package for blocking actions with a specific eloquent model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "davidnadejdin",
            "email": "Davidnadejdin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/database": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.0",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^3.8 || ^4.0",
        "sempro/phpunit-pretty-print": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Envant\\EloquentLockable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Envant\\EloquentLockable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Envant\\EloquentLockable\\LockableServiceProvider"
            ]
        }
    }
}
